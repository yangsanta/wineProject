<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="coupon.model.CouponVO" table="coupon">

		<id name="c_key" column="c_key">

		</id>

		<property name="c_price" column="c_price" />
		<property name="c_status" column="c_status" />
		<property name="c_deadline" column="c_deadline" />
		<property name="m_no" column="m_no" />
		

	</class>
</hibernate-mapping>

